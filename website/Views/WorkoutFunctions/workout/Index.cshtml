@model vs.models.WorkoutFunctions.Workouts


@foreach (var workout in Model.Workoutlist)
{
    <ul>
        <li>
            @workout.Name
            <ul>
                @foreach (var excercise in workout.Exercises)
                {
                    <li>@excercise.Name</li>
                }
            </ul>
        </li>
    </ul>

}

@Html.ActionLink("Test", "GetWorkouts", "Workout")
<button class="test">TEST</button>


<script>
    $(document).ready(function () {
        $(".test").click(function () {
            console.log("ABC");
            testAjax();
        });
    });

    function testAjax() {
        $.ajax({
            method: "GET",
            url: "Umbraco/Surface/Workout/GetWorkouts/",
            dataType: "json",
            success: function (data) {
                console.log(data);
            }
        });
    }
</script>